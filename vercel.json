{
    "version": 2,
    "builds": [
      {
        "src": "api/emailapi.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/sendEmail",
        "dest": "/api/emailapi.js"
      },
      {
        "src": "/(.*)",
        "dest": "/api/emailapi.js"
      }
    ]
  }
  